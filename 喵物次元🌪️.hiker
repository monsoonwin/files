{"last_chapter_rule":".module-play-list-content&&a,-1&&Text.js:'更新至：'+input","pageList":[{"col_type":"movie_3","name":"通用解析","path":"lazy","rule":"var lazy = $('').lazyRule(() => {\n    try {\n        var html = JSON.parse(request(input, {\n            headers: {\n                Accept: 'text/html,application/xhtml+xml,application/xml;q=0.9,image/webp,image/apng,*/*;q=0.8,application/signed-exchange;v=b3;q=0.9'\n            }\n        }).match(/r player_.*?=(.*?)</)[1])\n        var url = html.url\n        if (html.encrypt == '1') {\n            url = unescape(url);\n        } else if (html.encrypt == '2') {\n            url = unescape(base64Decode(url));\n        }\n        if (/m3u8|mp4/.test(url)) {\n            return url\n        } else {\n            var json = JSON.parse(post('https://player.catwjx.biz/echodebug.php', {\n                body: {\n                    url: url\n                }\n            }))\n\n            var m3 = cacheM3u8(json.url);\n            return m3\n        }\n    } catch (e) {\n        log(e.message)\n        return \"video://\" + input\n    }\n\n})"},{"col_type":"movie_3","name":"二级页面","path":"ej","rule":"js:\nvar d = []\nMY_URL=MY_URL.replace('hiker://empty##','');\nvar html = fetch(MY_URL);\n\neval(JSON.parse(fetch('hiker://page/er', {})).rule);\neval(JSON.parse(fetch('hiker://page/lazy', {})).rule);\nvar playlist = [{\n    /*自定义:{\n        图标: 'https://hikerfans.com/tubiao/q/3.png',\n        正序颜色: '#32CD32',\n        逆序颜色: '#FA8072',\n        线路选中: '#87CEEB',       \n    },*/\n    类型: '在线观看',\n    线路: 'body&&.anthology-tab&&a',\n    选集: 'body&&.anthology-list-play:not(:matches(α))',\n    线路名称: 'a--.fa&&Text@@.replace(\"α\",\"\")',\n    选集列表: 'body&&a:not(:matches(α))',\n    排除: '',\n    //选集名称: 'Text',\n    //选集链接: 'a&&href',\n    //倒序:true,\n    解析: lazy,\n    推送: true,\n}, {\n    类型: '磁力下载',\n    //线路: '',\n    选集: 'body&&.sort-itemm',\n    //线路名称: 'Text@@.replace(\"β\",\"\")',\n    //选集列表: 'ul&&li',\n    选集名称: 'Text',\n   